<mapper namespace="adminReview">

<select id="reviewList" parameterType="hashmap" resultType="hashmap">
        <![CDATA[
               SELECT ORDER_NUMBER, REVIEW_NUMBER, MEMBER_ID, REVIEW_CONTENT, REVIEW_IMAGE,
                REVIEW_TITLE,REVIEW_SCORE, REVIEW_REGDATE
                FROM REVIEW 
                ORDER BY REVIEW_NUMBER DESC               
        ]]>
        </select>
        
        <delete id="reviewDelete" parameterType="hashmap">
                <![CDATA[
                        DELETE REVIEW
                        WHERE
                                REVIEW_NUMBER = #{REVIEW_NUMBER}
                ]]>
        </delete>
        
        </mapper>